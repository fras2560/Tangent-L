      <Math mode="inline" xml:id="p1.m51" tex="{n-1\choose m}={n\choose m}-{n-1\choose m-1}" text="binomial@(n - 1, m) = binomial@(n, m) - binomial@(n - 1, m - 1)" fragid="p1.m51">
