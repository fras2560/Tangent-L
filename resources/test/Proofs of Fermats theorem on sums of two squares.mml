      <Math mode="inline" xml:id="p1.m13" tex="\frac{a^{2}+b^{2}}{p^{2}+q^{2}}=\left(\frac{ap+bq}{p^{2}+q^{2}}\right)^{2}+%&#10;\left(\frac{aq-bp}{p^{2}+q^{2}}\right)^{2}" text="(a ^ 2 + b ^ 2) / (p ^ 2 + q ^ 2) = ((a * p + b * q) / (p ^ 2 + q ^ 2)) ^ 2 + ((a * q - b * p) / (p ^ 2 + q ^ 2)) ^ 2" fragid="p1.m13">
        <m:math alttext="\frac{a^{2}+b^{2}}{p^{2}+q^{2}}=\left(\frac{ap+bq}{p^{2}+q^{2}}\right)^{2}+%&#10;\left(\frac{aq-bp}{p^{2}+q^{2}}\right)^{2}" display="inline">
          <m:semantics>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:msup>
                    <m:mi>a</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>+</m:mo>
                  <m:msup>
                    <m:mi>b</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                </m:mrow>
                <m:mrow>
                  <m:msup>
                    <m:mi>p</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>+</m:mo>
                  <m:msup>
                    <m:mi>q</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                </m:mrow>
              </m:mfrac>
              <m:mo>=</m:mo>
              <m:mrow>
                <m:msup>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mfrac>
                      <m:mrow>
                        <m:mrow>
                          <m:mi>a</m:mi>
                          <m:mo>⁢</m:mo>
                          <m:mi>p</m:mi>
                        </m:mrow>
                        <m:mo>+</m:mo>
                        <m:mrow>
