                    </m:msup>
                    <m:mo>+</m:mo>
                    <m:msup>
                      <m:mi>b</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                  </m:mrow>
                  <m:mo>)</m:mo>
                </m:mrow>
                <m:mo>⁢</m:mo>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mrow>
                    <m:msup>
                      <m:mi>p</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:mo>+</m:mo>
                    <m:msup>
                      <m:mi>q</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                  </m:mrow>
                  <m:mo>)</m:mo>
                </m:mrow>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mrow>
                <m:msup>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mrow>
                      <m:mrow>
                        <m:mi>a</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mi>p</m:mi>
                      </m:mrow>
                      <m:mo>+</m:mo>
                      <m:mrow>
                        <m:mi>b</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mi>q</m:mi>
                      </m:mrow>
                    </m:mrow>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mo>+</m:mo>
                <m:msup>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mrow>
                      <m:mrow>
                        <m:mi>a</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mi>q</m:mi>
                      </m:mrow>
                      <m:mo>-</m:mo>
                      <m:mrow>
                        <m:mi>b</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mi>p</m:mi>
                      </m:mrow>
                    </m:mrow>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:mn>2</m:mn>
                </m:msup>
              </m:mrow>
            </m:mrow>
            <m:annotation encoding="application/x-tex">(a^{2}+b^{2})(p^{2}+q^{2})=(ap+bq)^{2}+(aq-bp)^{2}\,</m:annotation>
          </m:semantics>
        </m:math>
      </Math>
