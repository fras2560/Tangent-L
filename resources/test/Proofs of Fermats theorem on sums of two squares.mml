    <p>
      <Math mode="inline" xml:id="p1.m1" tex="p=x^{2}+y^{2}" text="p = x ^ 2 + y ^ 2" fragid="p1.m1">
        <m:math alttext="p=x^{2}+y^{2}" display="inline">
          <m:semantics>
            <m:mrow>
              <m:mi>p</m:mi>
              <m:mo>=</m:mo>
              <m:mrow>
                <m:msup>
                  <m:mi>x</m:mi>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mo>+</m:mo>
                <m:msup>
                  <m:mi>y</m:mi>
                  <m:mn>2</m:mn>
                </m:msup>
              </m:mrow>
            </m:mrow>
            <m:annotation encoding="application/x-tex">p=x^{2}+y^{2}</m:annotation>
          </m:semantics>
        </m:math>
      </Math>
