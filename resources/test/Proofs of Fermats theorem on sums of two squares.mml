      <Math mode="inline" xml:id="p1.m5" tex="(pb-aq)(pb+aq)=p^{2}b^{2}-a^{2}q^{2}=p^{2}(a^{2}+b^{2})-a^{2}(p^{2}+q^{2})." text="(p * b - a * q) * (p * b + a * q) = p ^ 2 * b ^ 2 - a ^ 2 * q ^ 2 = p ^ 2 * (a ^ 2 + b ^ 2) - a ^ 2 * (p ^ 2 + q ^ 2)" fragid="p1.m5">
        <m:math alttext="(pb-aq)(pb+aq)=p^{2}b^{2}-a^{2}q^{2}=p^{2}(a^{2}+b^{2})-a^{2}(p^{2}+q^{2})." display="inline">
          <m:semantics>
            <m:mrow>
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mrow>
