      <Math mode="inline" xml:id="p1.m16" tex="(a^{2}+b^{2})(q^{2}+p^{2})=(aq+bp)^{2}+(ap-bq)^{2}\," text="(a ^ 2 + b ^ 2) * (q ^ 2 + p ^ 2) = (a * q + b * p) ^ 2 + (a * p - b * q) ^ 2" fragid="p1.m16">
        <m:math alttext="(a^{2}+b^{2})(q^{2}+p^{2})=(aq+bp)^{2}+(ap-bq)^{2}\," display="inline">
          <m:semantics>
            <m:mrow>
