                      <m:mrow>
                        <m:mi>a</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mi>q</m:mi>
                      </m:mrow>
                    </m:mrow>
                    <m:mo>)</m:mo>
                  </m:mrow>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:mrow>
                  <m:mrow>
                    <m:msup>
                      <m:mi>p</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:mo>⁢</m:mo>
                    <m:msup>
                      <m:mi>b</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                  </m:mrow>
                  <m:mo>-</m:mo>
                  <m:mrow>
                    <m:msup>
                      <m:mi>a</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                    <m:mo>⁢</m:mo>
                    <m:msup>
                      <m:mi>q</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                  </m:mrow>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:mrow>
