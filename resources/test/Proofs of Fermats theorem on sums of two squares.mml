                          <m:mn>2</m:mn>
                        </m:msup>
                      </m:mrow>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:mrow>
                </m:mrow>
              </m:mrow>
              <m:mo>.</m:mo>
            </m:mrow>
            <m:annotation encoding="application/x-tex">(pb-aq)(pb+aq)=p^{2}b^{2}-a^{2}q^{2}=p^{2}(a^{2}+b^{2})-a^{2}(p^{2}+q^{2}).</m:annotation>
          </m:semantics>
        </m:math>
